---
import RootLayout from './RootLayout.astro';
import Navbar from 'src/Components/Navbar.astro';

export interface Props {
	title:string;
	class?:string;
	navbar_class?:string;
}

const {title, class:className, navbar_class, ...rest} = Astro.props;

---

<RootLayout class={className} title={title} {...rest}>
	<Navbar/>
	<slot/>
</RootLayout>


